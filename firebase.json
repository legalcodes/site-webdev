{
  "hosting": {
    "public": "publish",
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
      { "source": "/angular/:path*/**", "destination": "https://dart.dev/:path*/**", "type": 301 },
      { "source": "/angular", "destination": "https://dart.dev/", "type": 301 },
      { "source": "/angular/api", "destination": "https://dart.dev/api?package=angular", "type": 301 },
      { "source": "/angular/api/**", "destination": "https://dart.dev/api?package=angular", "type": 301 },
      { "source": "/api/angular/angular", "destination": "https://dart.dev/api/angular/angular/angular-library", "type": 301 },
      { "source": "/api/angular_components/angular_components", "destination": "https://dart.dev/api/angular_components/angular_components/angular_components-library", "type": 301 },
      { "source": "/api/angular2", "destination": "https://dart.dev/api?package=angular", "type": 301 },
      { "source": "/api/angular2/**", "destination": "https://dart.dev/api", "type": 301 },
      { "source": "/tutorials/get-data/fetch-data", "destination": "https://dart.dev/tutorials/web/fetch-data", "type": 301 },
      { "source": "/changelog", "destination": "https://dart.dev/reference/changelog", "type": 301 },

      { "source": "/guides/get-started", "destination": "https://dart.dev/tutorials/web/get-started", "type": 301 },
      { "source": "/guides", "destination": "https://dart.dev/guides", "type": 301 },
      { "source": "/tutorials", "destination": "https://dart.dev/tutorials/web", "type": 301 },
      { "source": "/guides", "destination": "https://dart.dev/guides", "type": 301 },
      { "source": "/guides/get-started", "destination": "https://dart.dev/tutorials/web/get-started", "type": 301 },
      { "source": "/guides/html-library-tour", "destination": "https://dart.dev/guides/libraries/library-tour#darthtml", "type": 301 },
      { "source": "/tools/dart2js", "destination": "https://dart.dev/tools/dart2js", "type": 301 },
      { "source": "/tools/dartdevc", "destination": "https://dart.dev/tools/dartdevc", "type": 301 },
      { "source": "/tools/dartdevc/faq", "destination": "https://dart.dev/tools/dartdevc/faq", "type": 301 },
      { "source": "/tools/sdk", "destination": "https://dart.dev/tools/sdk", "type": 301 },
      { "source": "/tools/sdk/archive", "destination": "https://dart.dev/tools/sdk/archive", "type": 301 },
      { "source": "/tools/webdev", "destination": "https://dart.dev/tools/webdev", "type": 301 },
      { "source": "/tools/webstorm", "destination": "https://dart.dev/tools/jetbrains-plugin", "type": 301 },
      { "source": "/tutorials", "destination": "https://dart.dev/tutorials", "type": 301 },
      { "source": "/tutorials/get-data", "destination": "https://dart.dev/tutorials/web/fetch-data", "type": 301 },
      { "source": "/tutorials/get-data/fetch-data", "destination": "https://dart.dev/tutorials/web/fetch-data", "type": 301 },
      { "source": "/tutorials/low-level-html", "destination": "https://dart.dev/tutorials/web/low-level-html", "type": 301 },
      { "source": "/tutorials/low-level-html/add-elements", "destination": "https://dart.dev/tutorials/web/low-level-html/add-elements", "type": 301 },
      { "source": "/tutorials/low-level-html/connect-dart-html", "destination": "https://dart.dev/tutorials/web/low-level-html/connect-dart-html", "type": 301 },
      { "source": "/tutorials/low-level-html/remove-elements", "destination": "https://dart.dev/tutorials/web/low-level-html/remove-elements", "type": 301 }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
